	public static void main(String[] args){
		float A = 1;
		float B = 3;
		
		String F ="/";
		switch (F) {
		case "+":
		    System.out.println(A+B);
		    break;
		  case "-":
		    System.out.println(A-B);
		    break;
		  case "*":
		    System.out.println(A*B);
		    break;
		  case "/":
			  float result = (A/B);
		    System.out.println(result);
		    break;
		  		}
				
	}	
