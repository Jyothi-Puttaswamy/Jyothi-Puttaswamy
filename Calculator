public static void main(String[] args){
		int A = 4;
		int B = 2;
		String F ="/";
		switch (F) {
		case "+":
		    System.out.println(A+B);
		    break;
		  case "-":
		    System.out.println(A-B);
		    break;
		  case "*":
		    System.out.println(A*B);
		    break;
		  case "/":
		    System.out.println(A/B);
		    break;
		  		}
				
	}
